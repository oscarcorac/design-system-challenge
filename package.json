{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "serve:dev": "vite",
    "serve:storybook": "storybook dev -p 6006",
    "check:lint": "eslint ./src --ext vue,ts,js --fix",
    "check:storybook:lint": "eslint ./packages --ext vue,ts,js --fix",
    "check:ts": "vue-tsc -p tsconfig.app.json",
    "check:storybook:ts": "vue-tsc -p tsconfig.storybook.json",
    "check:code": "npm run check:lint && npm run check:ts",
    "check:storybook:code": "npm run check:storybook:lint && npm run check:storybook:ts",
    "build:prod": "npm run check:code && vite build",
    "build:storybook": "npm run check:storybook:code && storybook build",
    "build:packages": "lerna run --parallel build",
    "publish:packages": "lerna publish from-package"
  },
  "dependencies": {
    "@oscarcorac/core-ui": "0.0.1",
    "@vueuse/core": "^10.11.0",
    "vue": "^3.4.29",
    "vue-router": "^4.4.0",
    "vuex": "4.1.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.2.6",
    "@storybook/builder-vite": "^8.2.6",
    "@storybook/core-common": "^8.2.6",
    "@storybook/vue3": "^8.2.6",
    "@storybook/vue3-vite": "^8.2.6",
    "@types/node": "^20.14.10",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "@vitejs/plugin-vue": "^5.0.5",
    "autoprefixer": "^10.4.19",
    "builtin-modules": "^4.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-vue": "^9.27.0",
    "lerna": "^8.1.7",
    "postcss": "^8.4.39",
    "prettier": "^3.3.3",
    "sass": "^1.77.8",
    "storybook": "^8.2.6",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.2.2",
    "vite": "^5.3.1",
    "vite-svg-loader": "^5.1.0",
    "vue-tsc": "^2.0.21"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
